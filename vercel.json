{
  "version": 2,
  "functions": {
    "server.js": {
      "includeFiles": [
        "styles.css",
        "script.js",
        "index.html",
        "images/**"
      ]
    }
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}